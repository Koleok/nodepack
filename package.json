{
  "name": "nodepack",
  "version": "1.0.0",
  "description": "Starter for node apps using babel and webpack",
  "main": "bundle.js",
  "scripts": {
    "start": "webpack && node ./build/build.js",
    "debug": "node-inspector & nodemon --debug .",
    "test": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./es6/tests/test",
    "test-unit": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./es6/tests/unit",
    "test-integration": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./es6/tests/integration",
    "watch": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./es6/tests/test --watch"
  },
  "author": "Kyle Chamberlain",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "chalk": "^1.1.1",
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.1",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "node-inspector": "^0.12.5",
    "webpack": "^1.12.9"
  }
}
